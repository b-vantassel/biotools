include mrna/search_filter_mixin
include qgrs/search_filter_mixin
include urich/search_filter_mixin
doctype html
html(ng-app='app')
head
    link(rel="shortcut icon", href="/images/favicon.ico")
    link(rel="stylesheet", href="http://bootswatch.com/superhero/bootstrap.css")
    link(rel="stylesheet", href="http://bootswatch.com/assets/css/bootswatch.min.css")
    link(rel="stylesheet", href="/stylesheets/core.css")
    block styles

    block title


  body
    .navbar.navbar-default.navbar-fixed-top(role='navigation')
      .navbar-header
        button.navbar-toggle.collapsed(type="button", data-toggle="collapse" data-target=".navbar-collapse")
          span.sr-only Toggle navigation
          span.icon-bar
          span.icon-bar
          span.icon-bar
        a.navbar-brand(href="/")
          img(style="max-width:25px; margin-top: -7px;margin-right:5px", src="/images/tools.png")
          | biotools
      .collapse.navbar-collapse.navbar-responsive-collapse
        ul.nav.navbar-nav
          li.dropdown
            a.dropdown-toggle#tools(href="#", data-toggle="dropdown")
              | Databases
              b.caret
            ul.dropdown-menu(aria-labelledby="databases")
              li: a(href="#") Chromosome Sequences
              li: a(href="/gui/mrna") mRNA +
              li: a(href="#") Genes +
              li: a(href="#") Homologene
              li: a(href="#") Taxons
          li.dropdown
            a.dropdown-toggle#tools(href="#", data-toggle="dropdown")
              | Tools
              b.caret
            ul.dropdown-menu(aria-labelledby="tools")
              li.divider
              li.dropdown-header Sequence Alignment
              li
                a(href="/alignment") Needle
              li.divider
              li.dropdown-header Motif Analysis
              li
                a(href="/qgrs/input") QGRS Mapping
              li
                a(href="#") U-Rich Mapping

          li.dropdown
            a.dropdown-toggle#tools(href="#", data-toggle="dropdown")
              | Analysis
              b.caret
            ul.dropdown-menu(aria-labelledby="tools")
              li.divider
              li.dropdown-header Correlation
              li
                a(href="/ugcorrelate") QGRS and U-Rich Elements

        ul.nav.navbar-nav.navbar-right
          li
            a(href="#")
              img(style="max-width:25px; margin-top: -7px;margin-right:20px", src="/images/config.png")

    block content


    script(src='https://code.jquery.com/jquery-1.10.2.min.js')
    script(src='http://bootswatch.com/bower_components/bootstrap/dist/js/bootstrap.min.js')
    script(src='http://bootswatch.com/assets/js/bootswatch.js')
    script(src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.0-rc.5/angular.min.js")
    script(src="http://code.highcharts.com/highcharts.js")
    script(src="http://code.highcharts.com/modules/exporting.js")
    block scripts
